<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodsystem">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/unilogo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodSystem">
        <activity android:name=".Payment"/>
        <activity android:name=".Cart_View"/>
        <activity android:name=".menu_view"/>
        <activity android:name=".CreditCard"/>
        <activity android:name=".Delivery"/>
        <activity android:name=".DeliveryMethod"/>
        <activity android:name=".MealPlan"/>
        <activity android:name=".Pickup"/>
        <activity android:name=".review"/>
        <activity android:name=".Registration"></activity>
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>